<h3>Editing Analysis</h3>

<%= form_for [@model, @analysis] do |f| %>
  <% if @analysis.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@analysis.errors.count, "error") %>
prohibited this user from being saved:</h2>
    <ul>
    <% @analysis.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
  <% end %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>